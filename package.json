{
  "author": "Chris Campbell <chris@launchbadge.com>",
  "description": "Svelvet PWA Boilerplate",
  "name": "svoilerplate",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "prepare": "snowpack --babel --optimize --clean --include 'src/**/*.{ts,svelte}'",
    "lint:css": "stylelint 'src/**/*.{ts,svelte}'",
    "lint:ts": "tsc --noEmit && eslint --fix 'src/**/*.{ts,svelte}'",
    "lint": "npm-run-all lint:css lint:ts",
    "svelvet": "svelvet svelvet --immutable --preprocess preprocess.config.js",
    "dev": "NODE_ENV=development npm-run-all lint svelvet",
    "build": "NODE_ENV=production npm-run-all lint svelvet"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@pyoner/svelte-types": "^3.4.4-2",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-typescript": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "babel-plugin-add-import-extension": "^1.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.27",
    "postcss-load-config": "^2.1.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.7.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "stylelint": "^13.2.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-csstree-validator": "^1.8.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-declaration-strict-value": "^1.1.8",
    "stylelint-no-indistinguishable-colors": "^1.3.0",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-use-logical": "^1.1.0",
    "stylelint-value-no-unknown-custom-properties": "^2.0.0",
    "svelte-preprocess": "^3.4.0",
    "svelvet": "^0.3.6",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "core-js": "3",
    "svelte": "^3.19.1"
  }
}
